tHealthChangeTime=Timer.GetTime()\r\n\tProperty=player.Health/player.MaxHealth\r\n\tif Property>=0.7 then\r\n\t\tHealthBar.Texture=HealthGreen\r\n\telseif Property>=0.3 then\r\n\t\tHealthBar.Texture=HealthOrange\r\n\telse\r\n\t\tHealthBar.Texture=HealthRed\r\n\tend\t\r\n\tHealthBar.Size=Vector2(OriginSize.x * Property,OriginSize.y)\r\nend\r\n\r\nplayer.OnHealthChange:Connect(healthChange)\r\n\r\nwhile true do\r\n\tTimerNow=Timer.GetTime()\r\n\tif player.HealthDisplayMode==Enum.HealthDisplayMode.Always then\r\n\t\tscript.Parent.Visible=true\r\n\telseif player.HealthDisplayMode==Enum.HealthDisplayMode.Never then\r\n\t\tscript.Parent.Visible=false\r\n\telseif player.HealthDisplayMode==Enum.HealthDisplayMode.OnHit then\r\n\t\tif TimerNow<=HealthChangeTime+0.5 and HealthChangeTime ~=0then\r\n\t\t\tscript.Parent.Visible=true\r\n\t\telse\r\n\t\t\tscript.Parent.Visible=false\r\n\t\tend\r\n\telse \r\n\t\tif TimerNow<=HealthChangeTime+2 and HealthChangeTime ~=0 then\r\n\t\t\tscript.Parent.Visible=true\r\n\t\telse\r\n\t\t\tscript.Parent.Visible=false\r\n\t\tend\r\n\tend\r\n\twait(0.01)\t\r\nend","m_scriptType":"kNormalScript"}}]},{"class":"cMeshObject","name":"Mesh","guid":[2982749034,852971505,2542616414,1167912122],"parentGuid":[2301495294,1274365006,3003438828,1864040556],"components":[{"id":0,"class":"sFullTransform","data":{"m_localPosition":[0.0,0.0,0.0],"m_localRotation":[0.5,0.5,0.5,-0.5],"m_localScale":1.0}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Mesh"}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sMeshInstanceComponent","data":{"m_mesh":{"m_guid":[2673417156,4233839806,2596720158,984946725],"m_revision":-1,"m_type":"kMesh","m_autoGenerated":false},"m_texture":{"m_guid":[566201226,2076986354,2238476796,3150962878],"m_revision":-1,"m_type":"kTexture","m_autoGenerated":false}}},{"id":15,"class":"sRigidBodyComponent","data":{"m_isStatic":true,"m_kinematic":false,"m_density":900.0,"m_frictionRate":0.4000000059604645,"m_rough