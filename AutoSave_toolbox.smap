},"m_corner":0}}]},{"class":"cScriptObject","name":"Script","guid":[2971467210,263146350,2238969912,967258592],"parentGuid":[2909511961,3416149687,3059430650,1882327379],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Script"}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":10,"class":"sLuaComponent","data":{"m_luaContent":"HealthBar=script.Parent.Health\nOriginSize = HealthBar.Size\nHealthRed = ResourceManager.GetTexture(\"Blood_Red\")\nHealthGreen = ResourceManager.GetTexture(\"Blood_Green\")\nHealthOrange = ResourceManager.GetTexture(\"Blood_Orange\")\nHealthOutline = ResourceManager.GetTexture(\"Blood_Outline\")\nHealthChangeTime = 0\nTimerNow = 0\nplayer = script.Parent.Parent\nfunction healthChange()\n\tHealthChangeTime = Timer.GetTime()\n\tProperty = player.Health / player.MaxHealth\n\tif Property >= 0.7 then\n\t\tHealthBar.Texture = HealthGreen\n\t\telseif Property >= 0.3 then\n\t\tHealthBar.Texture = HealthOrange\n\telse\n\t\tHealthBar.Texture = HealthRed\n\tend\n\tHealthBar.Size = Vector2(OriginSize.x * Property, OriginSize.y)\nend\n\nplayer.OnHealthChange:Connect(healthChange)\n\nhealthChange()\nscript.Parent.BackGround.Texture=HealthOutline\nwhile true do\n\tTimerNow = Timer.GetTime()\n\tif player.HealthDisplayMode == Enum.HealthDisplayMode.Always then\n\t\tscript.Parent.Visible = true\n\telseif player.HealthDisplayMode == Enum.HealthDisplayMode.Never then\n\t\tscript.Parent.Visible = false\n\telseif player.HealthDisplayMode == Enum.HealthDisplayMode.OnHit then\n\t\tif TimerNow <= HealthChangeTime + 0.5 and HealthChangeTime ~= 0then\n\t\t\tscript.Parent.Visible = true\n\t\telse\n\t\t\tscript.Parent.Visible = false\n\t\tend\n\telse\n\t\tif TimerNow <= HealthChangeTime + 2 and HealthChangeTime ~= 0 then\n\t\t\tscript.Parent.Visible = true\n\t\telse\n\t\t\tscript.Parent.Visible = false\n\t\tend\n\tend\n\twait(0.01)\nend","m_scriptType":"kNormalScript"}}]},{"class":"cLocalFolderObject","name":"StaticNPCs","guid":[2827413860,1863993111,2692273583,3388597092],"parentGuid":[3226327987,1174227081,3181934101,1100841928],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"StaticNPCs"}},{"id":40000,"class":"sScriptHelperComponent","data":{}}]}]}}