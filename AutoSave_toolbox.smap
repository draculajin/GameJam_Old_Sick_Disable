erNum>0 then\r\n\t\t\ttemp=container[i].CurrentMemberNum/(container[i].CurrentMaxMemberNum)\r\n\t\t\tif(temp<min)then\r\n\t\t\t\tteamTojoin={}\r\n\t\t\t\tmin=temp\r\n\t\t\t\ttable.insert(teamTojoin,container[i])\r\n\t\r\n\t\t\telseif temp==min then\r\n\t\t\t\ttable.insert(teamTojoin,container[i])\r\n\t\t\tend\r\n\t\tend\r\n\tend\t\r\n\tlocal a=1\r\n\tif #teamTojoin>0 then\r\n\t\ta=math.random(1,#teamTojoin)\r\n\t\treturn teamTojoin[a];\r\n\telse\r\n\t\treturn nil;\r\n\tend\r\nend\r\n\t\t\r\n","m_scriptType":"kNormalScript"}}]},{"class":"cUiScreenUiObject","name":"ScreenGUI","guid":[1573724318,2580826513,2532482330,1813952860],"parentGuid":[344576033,1668630255,2683907297,2226597768],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"ScreenGUI"}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":20,"class":"sRenderComponent","data":{}},{"id":21,"class":"sNetworkComponent","data":{"m_debug_EnableNetwork":true}},{"id":26,"class":"sUiScreenUiComponent","data":{"m_widgetEnable":true,"m_widgetVisible":false,"m_widgetSize":[0.0,0.0],"m_order":3,"m_angle":0.0,"m_anchor":"kAlignBottomLeft","m_anchorPercent":[0.0,0.0],"m_alignment":"kAlignBottomLeft","m_alignmentPercent":[0.0,0.0],"m_scale":1.0,"m_preSize":[2728.0,1410.0],"m_oldTreeLevelVersion":false,"m_acceptRaycast":true,"m_acceptRaycastWhenEdit":false}}]},{"class":"cUiTextObject","name":"Text","guid":[105489803,316360451,2459208428,272767973],"parentGuid":[1573724318,2580826513,2532482330,1813952860],"components":[{"id":0,"class":"sIdentityTransform","data":{}},{"id":2,"class":"sObjectDataComponent","data":{"m_name":"Text"}},{"id":40000,"class":"sScriptHelperComponent","data":{}},{"id":20,"class":"sRenderComponent","data":{}},{"id":21,"class":"sNetworkComponent","data":{"m_debug_EnableNetwork":true}},{"id":23,"class":"sUiTextComponent","data":{"m_widgetEnable":true,"m_widgetVisible":true,"m_widgetSize":[2729.966796875,1408.4239501953126],"m_or